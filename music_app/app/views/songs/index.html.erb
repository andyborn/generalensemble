<h1>Songs</h1>

<% @songs.each do |song| %>

  <div>
    <%= link_to image_tag(song.image.thumb.url), song_path(song) %>
  </div>
  <tr>
    <td><%= song.title %></td>
    <td>by <%=link_to song.user.artist_name, user_path(song.user)%></td>
  </tr>
  
<% end %>
</table>

<br />

<%= link_to 'New Song', new_song_path %>
